package operator;

public class BitOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num1 = 5; //00000101
		int num2 = 10;//00001010
		
		int result;
		
		result = num1 & num2;
		System.out.println(result);
		result = num1 | num2;
		System.out.println(result);
		
		int val =2;
		System.out.println(val << 2);
		System.out.println(val >> 1);
		
	}

}
