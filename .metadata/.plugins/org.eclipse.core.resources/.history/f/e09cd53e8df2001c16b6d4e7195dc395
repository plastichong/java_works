package type;

public class PrintFormat {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int year = 2022;
		System.out.printf("올해는 %d년 입니다", year);
		
		float weight = 63.7f;
		
		System.out.printf("그의 몸무게는 %.1fkg입니다", weight);
		
		
		String nick = "얼음 공주";
		System.out.printf("그녀의 별명은 %s 입니다",nick);
	}

}
